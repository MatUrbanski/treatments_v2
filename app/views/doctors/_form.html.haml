= simple_form_for @doctor do |f|
  .col-md-4
    = f.input :fullname
    = f.input :specialization
    = f.association :ordering_unit
    = field_set_tag t('.address')
    = f.simple_fields_for :address do |af|
      = af.input :street
      = af.input :city
      = af.input :zip_code
    = submit_button(f)
